#include <stdio.h>

int main() {
    int N, second_last_digit;


    printf("Enter a number: ");
    scanf("%d", &N);

    if (N < 10 && N > -10) {
        printf("The number doesn't have a second last digit.\n");
    } else {

        second_last_digit = (N / 10) % 10;

       
        if (second_last_digit < 0) {
            second_last_digit = -second_last_digit;
        }

  
        printf("The second last digit is: %d\n", second_last_digit);
    }

    return 0;
}
