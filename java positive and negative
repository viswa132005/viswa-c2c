package basics;

import java.util.Scanner;

public class array5 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b[]=new int[a];
		for(int i=0;i<a;i++) {
			b[i]=sc.nextInt();
		}
		int pos=0;
		int neg=0;
		for(int i=0;i<a;i++) {
			if(b[i]>0) {
				pos++;
			}
			else {
				neg++;
			}
		}
		System.out.println("pos "+pos);
		System.out.println("neg "+neg);

	}

}
